/* components/my-tab-control/my-tab-control.wxss */
.tab-control{
  display: flex;
  line-height: 88rpx;
  height: 88rpx;
  background: white;
}
.tab-control-item{
  flex:1;
  text-align: center;
}
.active{
  color:#ff5777;
}
.active text{
  border-bottom: 8rpx solid #ff5777;
  padding: 15rpx 15rpx;
}